<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Administrateur</from-outcome>
			<to-view-id>/listConge.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listConge.xhtml</display-name>
		<from-view-id>/listConge.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listConge</from-outcome>
			<to-view-id>/listConge.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listConge.xhtml</display-name>
		<from-view-id>/listConge.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listConge.xhtml</display-name>
		<from-view-id>/listConge.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listEmploye</from-outcome>
			<to-view-id>/listEmploye.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listEmploye.xhtml</display-name>
		<from-view-id>/listEmploye.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listEmploye.xhtml</display-name>
		<from-view-id>/listEmploye.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listEmploye</from-outcome>
			<to-view-id>/listEmploye.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listEmploye.xhtml</display-name>
		<from-view-id>/listEmploye.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>add</from-outcome>
			<to-view-id>/addEmploye.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listConge.xhtml</display-name>
		<from-view-id>/listConge.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>add</from-outcome>
			<to-view-id>/addConge.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addEmploye.xhtml</display-name>
		<from-view-id>/addEmploye.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Emp</from-outcome>
			<to-view-id>/listEmploye.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addConge.xhtml</display-name>
		<from-view-id>/addConge.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Cng</from-outcome>
			<to-view-id>/listConge.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
